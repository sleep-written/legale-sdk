{
  "type": "module",
  "name": "legale-sdk",
  "description": "An unnofficial legale API wrapper",
  "version": "0.2.3",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sleep-written/legale-sdk"
  },
  "scripts": {
    "test": "clear && ava",
    "clean": "rm -rf ./dist",
    "build": "node --run clean && node --run build:mjs && node --run build:def",
    "build:mjs": "bb-path-alias build ./tsconfig.build.json",
    "build:def": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "prepare": "node --run build"
  },
  "keywords": [
    "legale",
    "sdk",
    "typescript",
    "api",
    "ava"
  ],
  "devDependencies": {
    "@bleed-believer/path-alias": "^2.3.2",
    "@inquirer/prompts": "^7.8.3",
    "@types/node": "^24.1.0",
    "ava": "^6.4.1",
    "docs": "^0.3.2-canary.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  }
}
